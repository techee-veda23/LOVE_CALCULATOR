print("Hello ! Welcome to love calculator !\nEnter your names please-->")
name1 = input()
name2 = input()
print("Calculating your score...")
combined_name = name1 + name2
lower_name = combined_name.lower()
t = lower_name.count("t")
r = lower_name.count("r")
u = lower_name.count("u")
e = lower_name.count("e")
first_digit = t+r+u+e

l = lower_name.count("l")
o = lower_name.count("o")
v = lower_name.count("v")
e = lower_name.count("e")
second_digit = l+o+v+e

score = int(str(first_digit)) + int(str(second_digit))

if (score < 10) or (score > 90):
  print(f"Your score is {score}.You both together are like coke and mentos.")
elif (score <= 30) and (score >= 50):
  print(f"Your score is {score}.You're alright together")
else:
  print(f"Your score is {score}.")
